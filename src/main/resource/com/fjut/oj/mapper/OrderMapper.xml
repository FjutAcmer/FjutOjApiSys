<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fjut.oj.mapper.OrderMapper">
    <insert id="insertOrder" parameterType="com.fjut.oj.pojo.Order" useGeneratedKeys="true" keyProperty="order.id" keyColumn="id">
        INSERT INTO t_order
        (username, goodsId, acb, time, isCancel)
        VALUES (
            #{order.username},
            #{order.goodsId},
            #{order.acb},
            #{order.time},
            #{order.isCancel}
        )
    </insert>
</mapper>